    "@types/node": "20.11.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "8.56.0",
    "prettier": "3.2.4",
    "prisma": "5.8.1",
    "prisma-dbml-generator": "0.10.0",
    "prisma-docs-generator": "0.8.0",
    "prisma-json-schema-generator": "5.1.1",
