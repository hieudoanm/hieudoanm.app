{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"plugins": ["@typescript-eslint", "solid"],
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"eslint:recommended",
		"plugin:solid/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": { "@typescript-eslint/no-explicit-any": "warn" }
}
