{
  "name": "@express/socket",
  "version": "0.0.1",
  "description": "Express.js - Socket - Template",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "start:prod": "node .",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.ts"
  },
  "dependencies": {
    "axios": "1.6.6",
    "cors": "2.8.5",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "pino": "8.17.2",
    "pino-pretty": "10.3.1",
    "socket.io": "4.7.4",
    "tsoa": "6.0.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.6",
    "@types/supertest": "6.0.2",
    "@types/webpack-node-externals": "3.0.4",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "3.0.3",
    "prettier": "3.2.4",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "webpack": "5.90.0",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}
