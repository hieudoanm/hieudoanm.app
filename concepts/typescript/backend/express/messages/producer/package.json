{
  "name": "@messages/producer",
  "version": "0.0.1",
  "description": "Messages Producer",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml  && yarn docs:swagger:html && yarn docs:postman",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "docs:swagger:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Express.js - Messages - Producer'",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "start:dev": "kill-port 4000 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.10.1",
    "@grpc/proto-loader": "0.7.10",
    "amqplib": "0.10.3",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "helmet": "7.1.0",
    "ioredis": "5.3.2",
    "kafkajs": "2.2.4",
    "pino": "8.19.0",
    "pino-pretty": "10.3.1",
    "stompit": "1.0.0",
    "swagger-ui-express": "5.0.0",
    "tsoa": "6.0.1"
  },
  "devDependencies": {
    "@redocly/cli": "1.9.1",
    "@types/amqplib": "0.10.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.22",
    "@types/stompit": "0.26.6",
    "@types/swagger-ui-express": "4.1.6",
    "@types/webpack-node-externals": "3.0.4",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "3.1.0",
    "rimraf": "5.0.5",
    "swagger2-to-postmanv2": "2.1.0",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "keywords": [
    "ActiveMQ",
    "RabbitMQ"
  ]
}
