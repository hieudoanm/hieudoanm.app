		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@typescript-eslint/parser": "7.3.1",
		"@webpack-cli/generators": "3.0.7",
		"concurrently": "8.2.2",
		"eslint": "8.57.0",
		"format": "0.2.2",
		"graphql-tag": "2.12.6",
		"jest": "29.7.0",
