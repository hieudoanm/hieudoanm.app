{
  "name": "@nest/graphql",
  "version": "0.0.1",
  "description": "Nest.js - HTTP - GraphQL",
  "main": "dist/main",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "yarn docs:postman && yarn docs:html",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "kill-port 5000 && nest start --watch",
    "start:debug": "kill-port 5000 && nest start --debug --watch",
    "start:prod": "kill-port 5000 && node dist/main",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Nest.js - API - Template'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "4.9.5",
    "@nestjs/apollo": "12.0.11",
    "@nestjs/common": "10.3.0",
    "@nestjs/core": "10.3.0",
    "@nestjs/graphql": "12.0.11",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/swagger": "7.1.17",
    "graphql": "16.8.1",
    "helmet": "7.1.0",
    "js-yaml": "4.1.0",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.3.0",
    "@redocly/cli": "1.6.0",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "openapi-to-postmanv2": "4.18.0",
    "prettier": "3.1.1",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.5",
    "source-map-support": "0.5.21",
    "spectaql": "2.3.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
