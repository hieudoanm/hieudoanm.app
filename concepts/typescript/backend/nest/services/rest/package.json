{
  "name": "@nest/rest",
  "version": "0.0.1",
  "description": "Nest.js - HTTP - Rest",
  "main": "dist/main",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist && prisma generate",
    "build": "nest build",
    "postbuild": "yarn docs:postman && yarn docs:html",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "kill-port 3000 && nest start",
    "start:dev": "kill-port 3000 && nest start --watch",
    "start:debug": "kill-port 3000 && nest start --debug --watch",
    "start:prod": "node dist/main",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Nest.js - API - Template'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate": "prisma format && prisma generate && prisma migrate dev --name init"
  },
  "dependencies": {
    "@nestjs/common": "10.3.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/devtools-integration": "0.1.5",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.1",
    "@nestjs/swagger": "7.2.0",
    "@prisma/client": "5.8.1",
    "bcrypt": "5.1.1",
    "helmet": "7.1.0",
    "js-yaml": "4.1.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.1",
    "@redocly/cli": "1.8.0",
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.7",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.6",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "openapi-to-postmanv2": "4.19.0",
    "prettier": "3.2.4",
    "prisma": "5.8.1",
    "rimraf": "5.0.5",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
