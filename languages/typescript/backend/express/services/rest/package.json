{
  "name": "@express/rest",
  "version": "0.0.1",
  "description": "HTTP - REST",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/express-http-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist && yarn docs:swagger:html",
    "build": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "start:prod": "node .",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "docs:swagger:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Express.js - API - Template'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.ts",
    "test:newman": "newman run ./docs/newman/collection.json -e ./docs/newman/environment.json",
    "prisma:migrate": "prisma format && prisma generate && prisma migrate dev --name init",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.2.2",
    "@apollo/server": "4.9.5",
    "@elastic/elasticsearch": "8.10.0",
    "@graphql-tools/merge": "9.0.0",
    "@prisma/client": "5.6.0",
    "axios": "1.6.2",
    "bcrypt": "5.1.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "dotenv-expand": "10.0.0",
    "express": "4.18.2",
    "express-rate-limit": "7.1.4",
    "graphql": "16.8.1",
    "graphql-depth-limit": "1.1.0",
    "helmet": "7.1.0",
    "http-errors": "2.0.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "pino": "8.16.2",
    "pino-pretty": "10.2.3",
    "tsoa": "5.1.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@redocly/cli": "1.4.1",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.9",
    "@types/jsonwebtoken": "9.0.5",
    "@types/morgan": "1.9.9",
    "@types/node": "20.9.2",
    "@types/uuid": "9.0.7",
    "@types/webpack-node-externals": "3.0.4",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@webpack-cli/generators": "3.0.7",
    "concurrently": "8.2.2",
    "eslint": "8.54.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "newman": "6.0.0",
    "nodemon": "3.0.1",
    "openapi-to-postmanv2": "4.18.0",
    "prettier": "3.1.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.2",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}
