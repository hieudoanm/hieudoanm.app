{
  "name": "@nest/grpc",
  "version": "0.0.1",
  "description": "Nest.js - HTTP - gRPC",
  "main": "dist/main",
  "repository": "git@github.com:houseoftypescript/nest-http-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:html": "redocly bundle ./docs/swagger/swagger.json --output ./public/index.html --title 'Nest.js - gRPC - API'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.10",
    "@grpc/proto-loader": "0.7.10",
    "@nestjs/common": "10.2.8",
    "@nestjs/core": "10.2.8",
    "@nestjs/microservices": "10.2.8",
    "@nestjs/platform-express": "10.2.8",
    "@nestjs/swagger": "7.1.16",
    "helmet": "7.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.8",
    "@redocly/cli": "1.4.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.8",
    "@types/node": "20.9.1",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "openapi-to-postmanv2": "4.18.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
