{
  "name": "@ts/graphql-apollo",
  "version": "0.0.1",
  "description": "TypeScript - GraphQL - Apollo",
  "main": "dist/index",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "format": "prettier --cache --write .",
    "serve": "webpack serve --mode=development",
    "dev": "concurrently \"yarn codegen\" \"yarn webpack:watch\" \"yarn dev:nodemon\"",
    "dev:nodemon": "nodemon ./dist/main.js --watch dist",
    "webpack:watch": "webpack --progress --watch",
    "start": "kill-port 5000 && node ./dist/main",
    "prebuild": "yarn codegen",
    "build": "webpack --mode=production",
    "test": "jest",
    "lint": "eslint ./src --fix",
    "codegen": "graphql-codegen --config codegen.ts || true"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.3.0",
    "@apollo/server": "4.10.5",
    "graphql": "16.9.0",
    "pino": "9.3.2"
  },
  "devDependencies": {
    "@eslint/js": "9.8.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.14",
    "@webpack-cli/generators": "3.0.7",
    "concurrently": "8.2.2",
    "eslint": "9.8.0",
    "graphql-tag": "2.12.6",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.4",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.0.1",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4"
  }
}
