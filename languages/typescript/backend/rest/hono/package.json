    "@types/node": "22.5.5",
    "eslint": "9.11.0",
    "globals": "15.9.0",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "prettier": "3.3.3",
    "prisma": "5.19.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
