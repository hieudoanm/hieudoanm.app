    "import json\n",
    "import pandas\n",
    "\n",
    "congress_df = pandas.read_csv(\"congress.csv\")\n",
    "congress_df = congress_df.fillna(value=\"\")\n",
    "congress = congress_df.to_dict(\"records\")\n",
    "\n",
    "file = open(\"congress.json\", \"w\")\n",
    "json.dump(congress, file, ensure_ascii=False, indent=2)"
   ]
