{
  "name": "@ts/rest-hono",
  "version": "0.0.1",
  "description": "TypeScript - Hono - Rest",
  "main": "index.js",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
  "license": "GPL-3.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "start": "vercel dev",
    "predev": "kill-port 8787 && docker compose up -d && prisma format && prisma generate",
    "dev": "dotenvx run -- wrangler dev api/index.ts",
    "deploy:vercel": "vercel",
    "lint": "eslint \"api/**/*.ts\" --fix",
    "format": "prisma format && prettier --cache --write .",
    "pretest": "prisma format && prisma generate",
    "test": "jest || true"
  },
  "dependencies": {
    "@prisma/client": "6.0.0",
    "hono": "4.6.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20241127.0",
    "@dotenvx/dotenvx": "1.25.1",
    "@eslint/js": "9.15.0",
    "@prisma/extension-accelerate": "1.2.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "eslint": "9.15.0",
    "globals": "15.12.0",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "prettier": "3.4.1",
    "prisma": "6.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "vercel": "39.1.2",
    "wrangler": "3.91.0"
  },
  "keywords": [
    "hono"
  ]
}
