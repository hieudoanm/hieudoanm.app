    "@nestjs/core": "10.4.13",
    "@nestjs/platform-express": "10.4.13",
    "@nestjs/swagger": "8.0.7",
    "@nestjs/throttler": "6.2.1",
    "axios": "1.7.8",
    "cache-manager": "6.3.0",
    "compression": "1.7.5",
    "cookie-parser": "1.4.7",
    "helmet": "8.0.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
