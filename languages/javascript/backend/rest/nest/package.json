    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "@types/supertest": "6.0.2",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.13.0",
    "jest": "29.7.0",
    "prettier": "3.4.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "webpack": "5.96.1",
    "yaml": "2.6.1"
